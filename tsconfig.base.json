{
  "compilerOptions": {
    "composite": false,
    "declaration": false,
    "declarationMap": false,
    "emitDeclarationOnly": false,
    "importHelpers": true,
    "isolatedModules": true,
    "lib": ["es2022", "esnext.asynciterable"],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "baseUrl": ".",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es2022",
    "paths": {
      "@agency-quest/nestjs-graphql": ["libraries/nestjs-graphql/src/index.ts"],
      "@agency-quest/nestjs-microservices": [
        "libraries/nestjs-microservices/src/index.ts"
      ],
      "@agency-quest/nestjs-mongodb": ["libraries/nestjs-mongodb/src/index.ts"],
      "@agency-quest/nestjs-users-service": [
        "microservices/users/src/index.ts"
      ],
      "@agency-quest/ng-apollo": ["libraries/ng-apollo/src/index.ts"],
      "@agency-quest/ng-cdk": ["libraries/ng-cdk/src/index.ts"],
      "@agency-quest/ng-landing-page": [
        "libraries/ng-landing-page/src/index.ts"
      ],
      "@agency-quest/ng-storybook": ["libraries/ng-storybook/src/index.ts"],
      "@agency-quest/sdk": ["libraries/sdk/src/index.ts"],
      "@agency-quest/storybook": ["domains/design-system/src/index.ts"]
    },
    "allowSyntheticDefaultImports": true
  }
}
